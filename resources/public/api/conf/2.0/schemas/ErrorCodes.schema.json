{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties":
  {
    "code" : {"$ref": "#/definitions/error-codeType"},
    "message" : {"type" : "string"}
  },
  "required" : ["code", "message"],
  "definitions":
  {
    "error-codeType" :
    {
      "type" : "string",
      "enum" :
      [
        "BAD_REQUEST",
        "UNAUTHORIZED",
        "STATUS_UNAVAILABLE",
        "RTI_NOT_FOUND",
        "INTERNAL_SERVER_ERROR",
        "SERVICE_UNAVAILABLE"
      ]
    }
  }
}
